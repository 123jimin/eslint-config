{
  "name": "@jiminp/eslint-config",
  "description": "JiminP's personal ESLint configuration.",
  "version": "1.1.3",
  "type": "module",
  "sideEffects": false,
  "main": "./src/index.js",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./ts": {
      "types": "./src/index.typescript.d.ts",
      "default": "./src/index.typescript.js"
    },
    "./js": {
      "types": "./src/index.javascript.d.ts",
      "default": "./src/index.javascript.js"
    },
    "./strict": {
      "types": "./src/strict.d.ts",
      "default": "./src/strict.js"
    },
    "./style": {
      "types": "./src/style.d.ts",
      "default": "./src/style.js"
    },
    "./package.json": "./package.json"
  },
  "author": "Jimin Park (https://github.com/123jimin/)",
  "homepage": "https://github.com/123jimin/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/123jimin/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/123jimin/eslint-config/issues"
  },
  "keywords": [
    "eslint-config"
  ],
  "license": "Unlicense",
  "engines": {
    "node": ">=18.18.0"
  },
  "peerDependencies": {
    "@eslint/js": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.8.0",
    "eslint": "^10.0.0",
    "typescript-eslint": "^8.55.0"
  },
  "peerDependenciesMeta": {
    "typescript-eslint": {
      "optional": true
    },
    "@stylistic/eslint-plugin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.9.0",
    "eslint": "^10.0.0",
    "typescript-eslint": "^8.56.0"
  }
}